<view class="container">
  <view class="top" style='background: url("data:image/png;base64,") no-repeat;'>
    <!-- <image bindtap="bindViewTap" class="userinfo-avatar" src="{{userInfo.avatarUrl}}" mode="cover"></image> -->
    <view class="img">
      <open-data type="userAvatarUrl"></open-data>
    </view>
  </view>
  <view class="obj">
    <view>
        <text>{{obj.SignIn}}</text>
        <text class="unit">天</text>
        <text class="bottom">连续签到</text>
    </view>
    <view>
      <text>{{obj.renren}}</text>
      <text class="bottom">人人币</text>
    </view>
     <view>
      <text>{{obj.InviteNum}}</text>
      <text class="bottom">邀请人数</text>
    </view>
  </view>
  <view class="baby">
    <view class="mybaby" bindtap='navigateTo'>
      <text>我的宝贝</text>
      <image src="../../img/right.png" class="icon r"></image>
      <text class="r">我领到的宝贝</text>
    </view>
    <view class='goods'>
      <view id="1" bindtap='navigateTo'>
          <image src="img/elivery.png" mode='widthFix'></image>
          <text>待发货</text>
      </view>
      <view bindtap='navigateTo'>
          <image src="img/shipped.png" mode='widthFix'></image>
          <text>已发货</text>
      </view>
    </view>
  </view>
  <view class="list">
    <view class="list_item" wx:for="{{list}}" wx:key="id" bindtap="navigateToRen" id="{{index+1}}">
      <image src="{{item.src}}" mode='widthFix' class="img"></image>
      <text>{{item.name}}</text>
      <image src="../../img/right.png" class="icon r"></image>
    </view>
    <button open-type='contact' class="list_item_btn">
      <image src="img/cus.png" mode='widthFix' class="img"></image>
      <text>联系客服</text>
      <image src="../../img/right.png" class="icon r"></image>
    </button>
  </view>
     
</view>

